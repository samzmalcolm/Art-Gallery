        <servlet-name>login_check</servlet-name>
        <url-pattern>/login_check</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>changeProfileImage</servlet-name>
        <url-pattern>/changeProfileImage</url-pattern>
