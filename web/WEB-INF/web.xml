    </servlet>
    <servlet>
        <servlet-name>login_check</servlet-name>
        <servlet-class>login_check</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>changeProfileImage</servlet-name>
        <servlet-class>changeProfileImage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addArt</servlet-name>
        <servlet-class>addArt</servlet-class>
