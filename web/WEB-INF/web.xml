        <servlet-class>artistsPage</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>buyerDashboard</servlet-name>
        <servlet-class>buyerDashboard</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginCheck</servlet-name>
        <url-pattern>/LoginCheck</url-pattern>
    </servlet-mapping>
